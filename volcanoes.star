# Plot volcanic eruptions on a world map

WIDTH = 64
HALF_W = WIDTH // 2
HEIGHT = 32
HALF_H = HEIGHT // 2
HDIV = 360 / WIDTH
HALF_HDIV = HDIV / 2

def main():

    FRAMES = [
        render.Text(r[0], font="tom-thumb", color="#fff")
        for r in DATA[1:]
    ]

    LATITUDES = [
        (r[2] - HALF_H) * (180 / HEIGHT) + ( 180 / HEIGHT / 2)
        for r in DATA[1:]
    ]

    LONGITUDES = [
        (r[3] - HALF_W) * HDIV + HALF_HDIV
        for r in DATA[1:]
    ]

    return render.Root(
        delay = 500,
        child = render.Stack(
            children = [
                # The map
                render.Image(src=MAP),
                # Box background for year label
                render.Padding(
                    pad = (0, 27, 0, 0),
                    child = render.Box(
                        color="#000a",
                        width=28,
                        height=5
                    )
                ),
                # Year label
                render.Padding(
                    pad = (0, 27, 0, 0),
                    child = render.Animation(FRAMES)
                    #child = render.Text("4300 BC", font="tom-thumb", color="#fff")
                )
                
            ]
        )
    )


